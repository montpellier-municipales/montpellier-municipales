{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "qwik check-client src dist && vite build -c adapters/static/vite.config.ts && node scripts/generate-sitemap.mjs",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "data:nomenclatures": "tsx scripts/build-nomenclatures.ts",
    "data:budget": "tsx scripts/process-budget.ts",
    "data:loans": "tsx scripts/process-loans.ts",
    "data:apcp": "tsx scripts/process-apcp.ts",
    "data:population": "tsx scripts/extract-population.ts",
    "data:all": "npm run data:nomenclatures && npm run data:budget && npm run data:loans && npm run data:apcp && npm run data:population"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.17.2",
    "@builder.io/qwik-city": "^1.17.2",
    "@eslint/js": "9.39.1",
    "@inlang/paraglide-js": "2.7.0",
    "@qwik-ui/headless": "^0.7.4",
    "@qwikest/icons": "^0.0.13",
    "@tanstack/qwik-table": "^8.21.3",
    "@tanstack/table-core": "^8.21.3",
    "@types/marked": "5.0.2",
    "@types/node": "25.0.0",
    "@vanilla-extract/css": "^1.17.5",
    "@vanilla-extract/vite-plugin": "^5.1.3",
    "chart.js": "^4.5.1",
    "compiled-i18n": "^1.2.1",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.17.2",
    "fast-xml-parser": "^5.3.3",
    "globals": "16.4.0",
    "gray-matter": "^4.0.3",
    "marked": "^17.0.1",
    "prettier": "3.6.2",
    "qwik-speak": "^0.24.0",
    "tsx": "^4.21.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "5.2.0",
    "undici": "^7.16.0",
    "vite": "7.1.11",
    "vite-tsconfig-paths": "^4.2.1",
    "zod": "^4.1.13"
  }
}
